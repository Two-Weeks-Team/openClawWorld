{
  "name": "openclawworld",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "pnpm": {
    "overrides": {
      "uWebSockets.js": "https://codeload.github.com/uNetworking/uWebSockets.js/tar.gz/fcfc622a4286909593b7f390056d89e0ca3b56b9",
      "minimatch@<10.2.1": "10.2.2",
      "ajv@>=6.0.0 <6.14.0": "6.14.0",
      "bn.js@>=4.0.0 <4.12.3": "4.12.3",
      "qs@>=6.7.0 <6.14.2": "6.14.2"
    }
  },
  "scripts": {
    "sync-maps": "node scripts/sync-maps.mjs",
    "verify:maps": "tsx scripts/verify-map-stack-consistency.ts",
    "verify:map-change": "pnpm sync-maps && pnpm verify:maps && pnpm test",
    "build": "pnpm run sync-maps && pnpm -r build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "dev:server": "pnpm --filter @openclawworld/server dev",
    "dev:client": "pnpm --filter @openclawworld/client dev",
    "load-test": "pnpm load-test:http && pnpm load-test:ws",
    "load-test:http": "tsx scripts/load-test.ts",
    "load-test:ws": "tsx scripts/load-test-ws.ts",
    "resident-agent-loop": "tsx scripts/resident-agent-loop.ts",
    "extract:assets": "python tools/extract_assets/extract.py --all",
    "generate:openapi-json": "tsx scripts/extract-openapi-json.ts",
    "generate:client": "pnpm generate:openapi-json && orval",
    "generate:tools": "pnpm --filter @openclawworld/plugin generate",
    "generate:commands": "pnpm --filter @openclawworld/plugin generate:commands",
    "generate:schemas": "tsx scripts/generate-schemas.ts",
    "generate": "pnpm generate:tools && pnpm generate:commands && pnpm generate:schemas && pnpm generate:client",
    "generate:openapi": "tsx scripts/generate-openapi.ts",
    "test:fuzz": "bash scripts/schemathesis-fuzz.sh",
    "validate:openapi": "tsx scripts/validate-openapi.ts",
    "ecosystem": "tsx packages/ecosystem/src/index.ts",
    "ecosystem:start": "tsx packages/ecosystem/src/index.ts start",
    "ecosystem:status": "tsx packages/ecosystem/src/index.ts status"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.1",
    "@colyseus/sdk": "0.17",
    "@eslint/js": "^10.0.1",
    "@kayahr/tiled": "^0.0.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "orval": "^8.4.1",
    "prettier": "^3.0.0",
    "tsx": "^4.7.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  }
}
